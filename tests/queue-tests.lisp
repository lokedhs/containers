(fiveam:test queue-with-precomputed-block
  (let ((q (make-blocking-queue)))
    (loop
       with last-value = 1
       for cmd in *queue-test-block*
       for i from 2
       do (ecase (car cmd)
            (push (queue-push q i))
            (pop (let ((value (queue-pop q)))
                   (fiveam:is (> value last-value))
                   (setq last-value value)))))
    (loop
       until (empty-p q)
       do (fiveam:is (plusp (queue-pop q))))))
